CommonBehavior.Endpoints=tcp -p 10000


# This property is used by the clients to connect to IceStorm.
TopicManager.Proxy=IceStorm/TopicManager:default -p 9999


joystickUniversal.robot_id = 2

JoystickAdapter=JoystickAdapter
joystickUniversal.Device = /dev/input/js0
joystickUniversal.NumAxes = 4
joystickUniversal.Axis_0 = turn, 0, -1, 1, false, 0.2  # degrees per second
joystickUniversal.Axis_1 = advance, 1, -1000, 1000,  true, 300 # mm por segundo
joystickUniversal.Axis_2 = tilt, 2, -16, 16, true, 300  #front-left movement
joystickUniversal.Axis_3 = side, 3, -700, 700, false, 300  #front-left movement



