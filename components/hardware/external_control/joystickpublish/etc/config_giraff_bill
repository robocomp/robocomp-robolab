CommonBehavior.Endpoints=tcp -p 11200


# This property is used by the clients to connect to IceStorm.
TopicManager.Proxy=IceStorm/TopicManager:default -p 9999


JoystickAdapter=JoystickAdapter
joystickUniversal.Device = /dev/input/js1
joystickUniversal.NumAxes = 5
joystickUniversal.Axis_0 = turn, 0, -1, 1, false, 0.2  # degrees per second
joystickUniversal.Axis_1 = advance, 1, -1000, 1000,  true, 200  # mm por segundo
joystickUniversal.Axis_2 = caca, 2, -1, 1, false, 0.01  #front-left movement
joystickUniversal.Axis_3 = bill_advance, 4, -10, 10, true, 0.01  #front-left movement
joystickUniversal.Axis_4 = bill_rotate, 3, -1, 1, false, 0.01  #front-left movement



Ice.Warn.Connections=0
Ice.Trace.Network=0
Ice.Trace.Protocol=0
